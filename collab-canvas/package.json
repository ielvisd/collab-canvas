{
  "name": "collab-canvas",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "predev": "node scripts/check-port.js",
    "dev": "npm run predev && nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "type-check": "vue-tsc --noEmit",
    "type-check:watch": "vue-tsc --noEmit --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:type": "eslint . --ext .ts,.vue",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest --config vitest.e2e.config.ts",
    "test:e2e:ui": "vitest --config vitest.e2e.config.ts --ui",
    "test:e2e:run": "vitest run --config vitest.e2e.config.ts",
    "test:e2e:browser": "vitest --config vitest.e2e.config.ts --reporter=verbose",
    "test:e2e:headed": "vitest --config vitest.e2e.config.ts --reporter=verbose --run",
    "test:visual": "vitest run tests/e2e/browser-visual.spec.ts --reporter=verbose",
    "test:browser": "node browser-test.js",
    "test:multi-user": "vitest run tests/e2e/multi-user-sync.spec.ts --reporter=verbose",
    "test:multi-user-browser": "node multi-user-test.js",
    "test:presence": "node presence-test.js"
  },
  "dependencies": {
    "@ai-sdk/vue": "^2.0.72",
    "@googlemaps/markerclusterer": "^2.6.2",
    "@nuxt/eslint": "1.9.0",
    "@nuxt/image": "1.11.0",
    "@nuxt/scripts": "0.12.2",
    "@nuxt/ui": "4.0.1",
    "@nuxthub/core": "^0.9.0",
    "@supabase/supabase-js": "^2.75.1",
    "@unhead/vue": "^2.0.19",
    "ai": "^5.0.72",
    "eslint": "^9.37.0",
    "konva": "^10.0.2",
    "nuxt": "^4.1.3",
    "nuxt-emoji-picker": "1.2.1",
    "typescript": "^5.9.3",
    "vue": "^3.5.22",
    "vue-konva": "^3.2.6",
    "vue-router": "^4.5.1",
    "workers-ai-provider": "^2.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.3",
    "@nuxt/test-utils": "3.19.2",
    "@tailwindcss/typography": "^0.5.19",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "nuxt-mcp": "^0.2.4",
    "playwright-core": "^1.56.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.14",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.1"
  }
}
